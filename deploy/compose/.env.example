# PostGIS
POSTGRES_DB=gis
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
PG_SCHEMA=shakir

# GeoServer
GEOSERVER_PORT=8080

# Kafka
KAFKA_CLUSTER_ID=abcdefghijklmnopqrstuvwxyz

# App-level
GEOSERVER_URL=http://localhost:8080/geoserver
REDIS_ADDR=localhost:6379
# Use 29092 for local run, and 9092 for Docker
KAFKA_BROKERS=localhost:29092
KAFKA_TOPIC=spatial-updates

LOG_LEVEL=debug

# Caching
CACHE_OP_TIMEOUT=250ms
CACHE_TTL_DEFAULT=60s
CACHE_TTL_OVERRIDES=demo:NR_polygon=2m,roads=30s
CACHE_FILL_MAX_WORKERS=8
CACHE_FILL_QUEUE=64

INVALIDATION_ENABLED=false
# If empty, producers get parent resolution from H3_RES-1
H3_PARTITION_RES=

# H3
H3_RES=8
H3_RES_MIN=8
H3_RES_MAX=8

# Metrics
METRICS_ENABLED=false
METRICS_ADDR=:9090
METRICS_PATH=/metrics
